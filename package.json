    "build": "webpack --mode production --config webpack.prod.js",
    "test": "npm run  lint",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint ./src/**/*.js",
    "lint:css": "stylelint './src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topotal/fe-base.git"
  },
